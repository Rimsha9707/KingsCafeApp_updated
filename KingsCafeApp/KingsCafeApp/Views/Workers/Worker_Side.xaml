             x:Class="KingsCafeApp.Views.Workers.Worker_Side"
             xmlns:local="clr-namespace:KingsCafeApp.Views.Workers"
            NavigationPage.HasNavigationBar="False"
            BarBackgroundColor="AntiqueWhite"
            SelectedTabColor="Black"
            UnselectedTabColor="{StaticResource Primary}"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core" xmlns:local1="clr-namespace:KingsCafeApp.Views.AWR"
            android:TabbedPage.ToolbarPlacement="Top"
            Title="Worker Home">
    <TabbedPage.ToolbarItems>
        <ToolbarItem  Text="LogOut" Clicked="ToolbarItem_Clicked"></ToolbarItem>
        <ToolbarItem IconImageSource="ic_Person.png"  x:Name="Profile" Clicked="Profile_Clicked"></ToolbarItem>
    </TabbedPage.ToolbarItems>
    <local1:Manage_Orders Title="Orders" IconImageSource="ic_order.png"/>
    <local:Worker_Home Title="Home" IconImageSource="ic_people.png"/>
